<h3 class="ui header">{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.номер
      class=(if (v-get validationModel "номер" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.номер-caption")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-номер=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "номер" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.датаНачала-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаНачала
      class=(if (v-get validationModel "датаНачала" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-датаНачала=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаНачала" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.датаОкончания-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаОкончания
      class=(if (v-get validationModel "датаОкончания" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-датаОкончания=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаОкончания" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.организация
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.организация-caption")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.склад
      class=(if (v-get validationModel "склад" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.склад-caption")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-склад=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "склад" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.контрагент
      class=(if (v-get validationModel "контрагент" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.контрагент-caption")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-контрагент=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "контрагент" "message")}}
  </div>
  <div class="field" data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-состОтгрузки="true">
    <label>{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.состОтгрузки-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-avtomatizaciya-zakupok2-сост-поставок")
      value=model.состОтгрузки
      class=(if (v-get validationModel "состОтгрузки" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "состОтгрузки" "message")}}
  </div>
  <div class="field" data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-состояниеОплаты="true">
    <label>{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.состояниеОплаты-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-avtomatizaciya-zakupok2-сост-оплаты")
      value=model.состояниеОплаты
      class=(if (v-get validationModel "состояниеОплаты" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "состояниеОплаты" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.суммаВклНДС-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.суммаВклНДС
      class=(if (v-get validationModel "суммаВклНДС" "isInvalid") "error")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-суммаВклНДС=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаВклНДС" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.суммаДокумента
      class=(if (v-get validationModel "суммаДокумента" "isInvalid") "error")
      label=(t "forms.i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e.суммаДокумента-caption")
      data-test-i-i-s-avtomatizaciya-zakupok2-отчет-о-закупках-e-суммаДокумента=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаДокумента" "message")}}
  </div>
</form>
